{
  "name": "my-webpack-tool",
  "version": "1.0.0",
  "description": " 我的 webpack 构建",
  "main": "index.js",
  "scripts": {
    "dev": "echo \"请使用 dev:desktop 或者 dev:mobile 命令来进行开发模式的启动\"",
    "dev:desktop": "node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --host 0.0.0.0 --port 1025 --config ./config/dev/desktop-config.js",
    "dev:mobile": "node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --host 0.0.0.0 --port 1024 --config ./config/dev/mobile-config.js",
    "build": "echo \"请使用 build:desktop 或者 build:mobile 命令来进行开发模式的启动\"",
    "build:desktop": "webpack -p --config ./config/prod/desktop-config.js",
    "build:mobile": "webpack -p --config ./config/prod/mobile-config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:F-happy/my-webpack-tool.git"
  },
  "author": "fuhuixiang",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.4.2",
    "antd-mobile": "^0.9.8",
    "es6-promise": "^4.0.5",
    "moment": "latest",
    "rc-form": "^1.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "sass-loader": "^4.0.2",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "precss": "^1.4.0",
    "babel-plugin-import": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "postcss-pxtorem": "^3.3.1",
    "react-native": "^0.36.0"
  }
}
