{
  "name": "my-webpack-tool",
  "version": "2.0.0",
  "description": " 我的 webpack 构建",
  "main": "index.js",
  "scripts": {
    "dev": "echo \"请使用 dev:desktop 或者 dev:mobile 命令来进行开发模式的启动\"",
    "dev:desktop": "node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --host 0.0.0.0 --port 1025 --config ./config/dev/desktop-config.js",
    "dev:mobile": "node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --host 0.0.0.0 --port 1024 --config ./config/dev/mobile-config.js",
    "build": "echo \"请使用 build:desktop 或者 build:mobile 命令来进行开发模式的启动\"",
    "build:desktop": "webpack -p --config ./config/prod/desktop-config.js",
    "build:mobile": "webpack -p --config ./config/prod/mobile-config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:F-happy/my-webpack-tool.git"
  },
  "author": "fuhuixiang",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.12.0",
    "antd-mobile": "^1.4.2",
    "es6-promise": "^4.1.1",
    "moment": "^2.18.1",
    "rc-form": "^1.4.3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "sass-loader": "^6.0.6",
    "string-replace-loader": "^1.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "babel-plugin-import": "^1.2.1",
    "html-webpack-plugin": "^2.29.0",
    "postcss-pxtorem": "^4.0.1",
    "react-native": "^0.46.1"
  }
}
